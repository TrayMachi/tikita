{
  "expo": {
    "name": "Tikita",
    "slug": "tikita",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/IconTikita.png",
    "scheme": "tikita",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/IconTikita.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tristanagra.tikita"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/IconTikita.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tristanagra.tikita"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "35a9d9a6-78ea-4d76-a576-9d578ee82f03"
      }
    },
    "owner": "tristanagra"
  }
}
